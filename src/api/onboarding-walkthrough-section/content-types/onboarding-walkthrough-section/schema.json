{
  "kind": "collectionType",
  "collectionName": "onboarding_walkthrough_sections",
  "info": {
    "singularName": "onboarding-walkthrough-section",
    "pluralName": "onboarding-walkthrough-sections",
    "displayName": "Walkthrough Section"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "sectionKey": {
      "type": "uid",
      "required": true
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "order": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "min": 1
    },
    "video": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "videoUrl": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "videoDescription": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "walkthrough": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::onboarding-walkthrough.onboarding-walkthrough",
      "inversedBy": "sections"
    },
    "additionalContent": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "onboarding.additional-content",
      "repeatable": false
    }
  }
}
