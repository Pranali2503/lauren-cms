{
  "kind": "collectionType",
  "collectionName": "learning_modules",
  "info": {
    "singularName": "learning-module",
    "pluralName": "learning-modules",
    "displayName": "Learning Module"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "summary": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "weekRange": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "order": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "estimatedDurationMinutes": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "heroImage": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "topic": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::learning-topic.learning-topic",
      "inversedBy": "modules"
    },
    "subtopics": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subtopic.subtopic",
      "mappedBy": "module"
    },
    "organization": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "isBaseTemplate": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
